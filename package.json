{
    "name": "eleventy-base-blog",
    "version": "5.0.2",
    "description": "A starter repository for a blog web site using the Eleventy static site generator.",
    "scripts": {
        "tailwind:dev": "npx tailwindcss build css/tailwind.css -o css/index.css -c tailwind.dev.config.js",
        "tailwind:prod": "npx tailwindcss build css/tailwind.css -o css/index.css -c tailwind.prod.config.js",
        "postcss:dev": "npx postcss css/index.css > css/index.min.css",
        "postcss:prod": "npx postcss --use cssnano --map false -o css/index.min.css css/index.css",
        "build:dev": "npm run tailwind:dev && npm run postcss:dev && eleventy",
        "build:prod": "npm run tailwind:prod && npm run postcss:prod && eleventy",
        "watch:dev": "npm run build:dev -- --watch",
        "serve:dev": "npm run build:dev -- --serve",
        "watch:prod": "npm run build:prod -- --watch",
        "serve:prod": "npm run build:prod -- --serve",
        "debug": "DEBUG=* eleventy"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/11ty/eleventy-base-blog.git"
    },
    "author": {
        "name": "Zach Leatherman",
        "email": "zachleatherman@gmail.com",
        "url": "https://zachleat.com/"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/11ty/eleventy-base-blog/issues"
    },
    "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
    "devDependencies": {
        "@11ty/eleventy": "^0.11.0",
        "@11ty/eleventy-navigation": "^0.1.3",
        "@11ty/eleventy-plugin-rss": "^1.0.7",
        "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
        "clean-css": "^4.2.3",
        "cssnano": "^4.1.10",
        "eleventy-plugin-toc": "^1.0.1",
        "html-minifier": "^4.0.0",
        "luxon": "^1.21.3",
        "markdown-it": "^8.4.2",
        "markdown-it-anchor": "^5.2.5",
        "postcss-cli": "^7.1.1",
        "prettier": "^2.0.5",
        "snowpack": "^2.5.3"
    },
    "dependencies": {
        "alpinejs": "^2.4.1",
        "tailwindcss": "^1.4.6"
    }
}
